{
  "rewrites": [
    {
      "source": "/privacy",
      "destination": "/privacy.html"
    },
    {
      "source": "/terms",
      "destination": "/terms.html"
    },
    {
      "source": "/auth/tiktok/callback",
      "destination": "/"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/config.js",
      "destination": "/config.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
